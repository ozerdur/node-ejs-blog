<%-  include('partials/header') -%>
<h1>Home</h1>
<p><%= content %></p>
<% posts.forEach((post)=>{%>
<h1><%= post.title %></h1>
<% target= "/posts/" + post.title.replace(" ", "-").toLowerCase(); %>
<p><%= post.content.substring(0,100)  + "..." %>
<a href= "/posts/<%= post.title %>">Read More</a>
</p>
<% }); %>
<%-  include('partials/footer') -%>
